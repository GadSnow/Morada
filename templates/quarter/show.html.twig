{% extends 'base.html.twig' %}

{% block title %}Quarter{% endblock %}

{% block body %}
    <h1>Quarter</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ quarter.id }}</td>
            </tr>
            <tr>
                <th>QuarterName</th>
                <td>{{ quarter.quarterName }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ quarter.createdAt ? quarter.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_quarter_index') }}">back to list</a>

    <a href="{{ path('app_quarter_edit', {'id': quarter.id}) }}">edit</a>

    {{ include('quarter/_delete_form.html.twig') }}
{% endblock %}
